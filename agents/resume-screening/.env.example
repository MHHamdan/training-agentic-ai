# API Keys - Hugging Face (Required for free models)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Observability (Required for production monitoring)
LANGCHAIN_API_KEY=your_langsmith_api_key_here

# Cloud Provider APIs (Optional - for premium models)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
COHERE_API_KEY=your_cohere_api_key_here
GROQ_API_KEY=your_groq_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here
XAI_API_KEY=your_xai_api_key_here
CEREBRAS_API_KEY=your_cerebras_api_key_here

# LangSmith Configuration
LANGCHAIN_PROJECT=resume-screening-agent-v2
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# Model Configuration
DEFAULT_MODEL=microsoft/phi-3-mini-4k-instruct
ENABLE_MODEL_COMPARISON=true
MAX_CONCURRENT_MODELS=3

# Provider Preferences
USE_CLOUD_APIS=false
PREFER_FREE_MODELS=true

# Performance Settings
MAX_FILE_SIZE_MB=10
PROCESSING_TIMEOUT_SECONDS=300
VECTOR_STORE_BATCH_SIZE=100

# ChromaDB Settings
CHROMA_PERSIST_DIRECTORY=./chroma_db

# Agent Settings
AGENT_PORT=8512